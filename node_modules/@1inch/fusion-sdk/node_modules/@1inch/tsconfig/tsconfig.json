{
  "compilerOptions": {
    "lib": [
      "ES2022"
    ],
    "module": "commonjs",
    "strict": true,
    "target": "ES2022",
    "resolveJsonModule": true,
    "declaration": true,
    "removeComments": true,
    "esModuleInterop": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "incremental": true,
    "paths": {},
    "skipLibCheck": true,
    "allowJs": true,
  }
}
